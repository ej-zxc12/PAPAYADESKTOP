{
  "name": "pledged-dashboard-electron",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:vite\" \"npm:dev:dev-electron\"",
    "dev:vite": "vite",
    "dev:dev-electron": "wait-on http://localhost:5173 && electron .",
    "build": "vite build",
    "start": "electron .",
    "lint": "echo \"No linter configured\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.7.2",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.0",
    "electron": "^28.2.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0",
    "wait-on": "^7.0.1"
  }
}
